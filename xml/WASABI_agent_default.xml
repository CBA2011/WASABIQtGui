		<info>
			<wasabi:secondary>
				<wasabi:parameter name="base_intensity" value="0.0"/>
				<wasabi:parameter name="lifetime" value="10.0"/>
				<wasabi:parameter name="decay" value="linear"/>
				<wasabi:polygon type="QUAD">
					<wasabi:vertex pleasure="0.0" arousal="1.0" dominance="0.0" intensity="1.0"/>
					<wasabi:vertex pleasure="0.5" arousal="1.0" dominance="0.0" intensity="0.0"/>
					<wasabi:vertex pleasure="0.5" arousal="0.0" dominance="0.0" intensity="0.0"/>
